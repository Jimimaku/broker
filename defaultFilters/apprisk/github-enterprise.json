[{
    "//": "used to get repo's teams list",
    "method": "GET",
    "path": "/repos/:owner/:repo/teams",
    "origin": "https://${GITHUB_TOKEN}@${GITHUB_API}"
  },
  {
    "//": "used to get team's members list",
    "method": "GET",
    "path": "/orgs/:username/teams/:team_slug/members",
    "origin": "https://${GITHUB_TOKEN}@${GITHUB_API}"
  },
  {
    "//": "used to get repo's contributors list",
    "method": "GET",
    "path": "/repos/:owner/:repo/contributors",
    "origin": "https://${GITHUB_TOKEN}@${GITHUB_API}"
  },
  {
    "//": "used to get repo's topics list",
    "method": "GET",
    "path": "/repos/:owner/:repo/topics",
    "origin": "https://${GITHUB_TOKEN}@${GITHUB_API}"
  }]